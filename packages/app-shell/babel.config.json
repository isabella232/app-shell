{
  "presets": [
    "@babel/preset-react",
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": 3,
        "targets": "> 0.25%, not dead"
      }
    ]
  ],
  "plugins": [
    [
      "babel-plugin-styled-components", {
        "displayName": "true",
        "namespace": "app-shell"
      }
    ]
  ],
  "env": {
    "test": {
      "plugins": [
        ["@babel/plugin-transform-runtime",
          {
            "absoluteRuntime": false,
            "corejs": false,
            "helpers": true,
            "regenerator": true,
            "useESModules": false
          }
        ]
      ]
    }
  }
}
